{
  "name": "my-discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot built with TypeScript",
  "main": "dist/bot.js",
  "scripts": {
    "start": "npx ts-node src/bot.ts",
    "build": "tsc",
    "postbuild": "xcopy /E /I /Y ./src/assets/matchups ./dist/assets/matchups",
    "watch": "tsc-watch --onSuccess \"ts-node src/bot.ts\"",
    "dev": "tsc-watch --onSuccess \"node dist/bot.js\"",
    "deployglobal": "ts-node ./scripts/deploy-commands.ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.18.0",
    "@sapphire/decorators": "^6.1.1",
    "@sapphire/discord.js-utilities": "^7.3.1",
    "@sapphire/framework": "^5.3.1",
    "@sapphire/pieces": "^4.3.1",
    "@sapphire/utilities": "^3.18.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.5.0",
    "color": "^4.2.3",
    "date-fns": "^4.1.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.1",
    "ms": "^2.1.3",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^5.0.1",
    "@types/color": "^4.2.0",
    "@types/node": "^16.18.119",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.12",
    "@types/sharp": "^0.31.1",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^4.6.2",
    "typescript": "^5.6.3"
  }
}
